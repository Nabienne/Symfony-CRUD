{% extends 'base.html.twig' %}

 
 
  {% block body %}

  <h1 style="text-align: center">Supprimer employer</h1>
      <table border=1px class="table table-dark">
  <thead>
  <th scope="col">Matricule</th>
  <th scope="col">Nom complet</th>
  <th scope="col">Date de naissance</th>
  <th scope="col">Salaire</th>
  <th scope="col">Service</th>
  <th scope="col">Supprimer</th>
  </thead>
  {% for employers in employe %}
  <tr>
  <td>{{employers.matricule}}</td>
  <td>{{employers.nomComplet}}</td>
  <td>{{employers.dateNaissance | date('d/m/Y')}}</td>
  <td>{{employers.salaire}}</td>
  <td>{{employers.service.libelle}}</td>
   <td>
  <a href="/service/{{employers.id}}/supprimer" class="btn btn-success ">supprimer</a>
  </td>
  </tr>
      
  {% endfor %}
      </table>
  {% endblock %}